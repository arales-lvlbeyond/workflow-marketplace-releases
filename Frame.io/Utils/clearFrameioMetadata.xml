<workflow xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://levelsbeyond.com/schema/workflow"
	xmlns:nimbus="http://levelsbeyond.com/schema/workflow/nimbus"
	xsi:schemaLocation="http://levelsbeyond.com/schema/workflow https://edge.reachengine.com/reachengine/api/workflows/schema/studio.xsd"
	id="clearFrameioMetadata"
	name="Clear Frame.io Metadata"
	executionLabelExpression="Clear Frame.io Metadata - ${object.name}"
	subjectDOClassName="AssetMaster"
	subjectQualifierExpression="${metadata.frameioID != null AND metadata.frameIOParentID != null}"
	showInUserInterface="true"
	group="Frame.io"
	adminOnly="true"
	sdkVersion="">

	<initialStepName>clear frameio metadata</initialStepName>
	
	<saveDataObjectStep name="clear frameio metadata"
		dataObjectExpression="${object.metadata}"
		jsonValuesDataDef="frameioMetadata"
		nextStep="end"
	/>
	
	<!-- ................................................... End Steps .................................................... -->
	<!-- success -->
	<noopStep name="end" />
	<!-- ................................................... Data Defs .................................................... -->
	<!-- ............ INPUT ............. -->
	<contextDataDef name="object"                       dataType="Data Object"        defaultDataExpression="${ subject }" />
	<contextDataDef name="frameioMetadata"              dataType="JSON">
		<defaultDataExpression>
			<![CDATA[
				{
					"frameioID" : "",
					"frameIOParentID" : ""
				}
			]]>
		</defaultDataExpression>
	</contextDataDef>
	<!-- ......... PROCESSING ........... -->
	
	
    
</workflow>