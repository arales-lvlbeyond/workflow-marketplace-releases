            <xsd:enumeration value="FireTv-v1.11"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="NonEmptyToken">
        <xsd:restriction base="xsd:token">
            <xsd:minLength value="1"/>
        </xsd:restriction>
    </xsd:simpleType>

